{
  "name": "@jeaks03/overseer",
  "version": "1.1.2",
  "description": "Just another TypeScript Back-End framework",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "repository": "https://github.com/paulcosma97/overseer",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "describe": "npm-scripts-info",
    "build": "run-s clean && run-p build:main",
    "build:main": "tsc -p tsconfig.json",
    "build:dev": "tsc -p tsconfig.dev.json",
    "test": "run-s build test:*",
    "test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "nyc --silent ava",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build test",
    "dev": "nodemon -e ts --watch src --exec \"run-s clean && tsc -p tsconfig.dev.json && node build/index.js overseer-dev\""
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "test": "Lint and unit test the project"
  },
  "engines": {
    "node": ">=8.9"
  },
  "devDependencies": {
    "ava": "1.0.0-beta.7",
    "codecov": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.0.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "opn-cli": "^4.0.0",
    "prettier": "^1.15.2",
    "standard-version": "^4.4.0",
    "trash-cli": "^1.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/**/*.spec.js"
    ],
    "sources": [
      "build/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "dependencies": {
    "@jeaks03/cached": "*",
    "@jeaks03/logger": "*",
    "@types/node": "^10.12.21",
    "axios": "^0.18.0",
    "deasync": "^0.1.14",
    "jsonwebtoken": "^8.4.0",
    "npmlog": "^4.1.2",
    "promisify": "0.0.3"
  }
}
